public String lastTwo(String str) {
   if (str.length() < 2) {
            return str; 
        }
        String lastTwoChars = str.substring(str.length() - 2); 
        String swapped = lastTwoChars.charAt(1) + "" + lastTwoChars.charAt(0);
        return str.substring(0, str.length() - 2) + swapped; 
    }

