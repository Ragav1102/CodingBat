public String deFront(String str) {    
 String result = "";
        if (str.length() > 0) {
            result += (str.charAt(0) == 'a') ? str.charAt(0) : "";
        }
        if (str.length() > 1) {
            result += (str.charAt(1) == 'b') ? str.charAt(1) : "";
        }
        if (str.length() > 2) {
            result += str.substring(2);
        }
        return result;
  } 

